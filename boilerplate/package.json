    "@semapps/backup": "0.4.0-alpha.1",
    "@sentry/node": "^5.16.1",
    "@sentry/utils": "^5.16.1",
    "dotenv-flow": "^3.2.0",
    "handlebars": "^4.7.7",
    "ioredis": "^4.17.3",
    "moleculer": "^0.14.18",
    "moleculer-bull": "0.2.8",
    "moleculer-cron": "^0.0.2",
    "moleculer-mail": "^1.2.5",
    "moleculer-repl": "^0.6.6",
    "nodemailer-sendgrid": "^1.0.3",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
