{
  "name": "activitypods-frontend",
  "version": "1.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@semapps/activitypub-components": "0.4.0-alpha.39",
    "@semapps/auth-provider": "0.4.0-alpha.39",
    "@semapps/date-components": "0.4.0-alpha.39",
    "@semapps/field-components": "0.4.0-alpha.39",
    "@semapps/geo-components": "0.4.0-alpha.39",
    "@semapps/input-components": "0.4.0-alpha.39",
    "@semapps/list-components": "0.4.0-alpha.39",
    "@semapps/semantic-data-provider": "0.4.0-alpha.39",
    "history": "4.10.1",
    "ra-language-english": "^3.17.0",
    "ra-language-french": "^3.17.0",
    "react": "^16.13.1",
    "react-admin": "^3.17.0",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^16.13.1",
    "react-sticky-box": "^0.9.3"
  },
  "devDependencies": {
    "prettier": "^2.3.2",
    "react-scripts": "3.4.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "prettier": "prettier --single-quote --print-width 120 --write '**/*.js'",
    "link-semapps-packages": "yarn link @semapps/activitypub-components @semapps/auth-provider @semapps/date-components @semapps/field-components @semapps/geo-components @semapps/input-components @semapps/list-components @semapps/markdown-components @semapps/semantic-data-provider",
    "unlink-semapps-packages": "yarn unlink @semapps/activitypub-components @semapps/auth-provider @semapps/date-components @semapps/field-components @semapps/geo-components @semapps/input-components @semapps/list-components @semapps/markdown-components @semapps/semantic-data-provider"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.js"
        ],
        "rules": {
          "jsx-a11y/anchor-is-valid": "off"
        }
      }
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
